  "scripts": {
    "build": "nest build",
    "debug:api": "nest start --debug --watch",
    "debug:worker": "SERVER_TYPE=worker nest start --debug --watch",
    "dev:api": "nest start --watch",
    "dev:worker": "SERVER_TYPE=worker nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"!src/database/migrations/db/**\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"!src/database/migrations/db/**\"",
    "lint": "eslint \"src/**/*.ts\" --ignore-pattern \"src/database/migrations/db/**\" --fix",
    "lint:check": "eslint \"src/**/*.ts\" --ignore-pattern \"src/database/migrations/db/**\"",
    "test": "jest",
    "tsn": "ts-node --project ./tsconfig.json --files -r tsconfig-paths/register",
    "type-check": "tsc --noEmit",
    "typeorm": "yarn tsn ./node_modules/typeorm/cli",
    "typeorm:create": "wrap () { yarn typeorm migration:create ./src/database/migrations/db/\"$@\" | cat; }; wrap",
    "typeorm:generate": "wrap () { yarn typeorm -d src/orm.config.ts migration:generate ./src/database/migrations/db/\"$@\" | cat; }; wrap",
    "typeorm:revert": "yarn typeorm -d src/orm.config.ts migration:revert",
    "typeorm:run": "yarn typeorm migration:run -d src/orm.config.ts"
  },
